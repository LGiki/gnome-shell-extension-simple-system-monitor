                              </object>
                            </child>

                            <child>
                              <object class="GtkEntry" id="item_separator">
                                <property name="visible">True</property>
                                <property name="can-focus">True</property>
                                <property name="text"></property>
                                <property name="valign">center</property>
                                <signal name="changed" handler="item_separator_changed" swapped="no" />
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>

                <!-- Refresh Interval -->
                <child>
                  <object class="GtkListBoxRow">
                    <property name="visible">True</property>
                    <property name="can-focus">True</property>

                    <child>
                      <object class="GtkBox">
                        <property name="visible">True</property>
                        <property name="can-focus">True</property>
                        <property name="margin-start">12</property>
                        <property name="margin-end">12</property>
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="orientation">vertical</property>

                        <child>
                          <object class="GtkBox">
                            <property name="visible">True</property>
                            <property name="can-focus">True</property>
                            <property name="spacing">32</property>

                            <child>
                              <object class="GtkLabel">
                                <property name="visible">True</property>
                                <property name="can-focus">False</property>
                                <property name="valign">center</property>
                                <property name="label" translatable="yes">Refresh Interval (second)</property>
                                <property name="xalign">0</property>
                                <property name="hexpand">1</property>
                              </object>
                            </child>

                            <child>
                              <object class="GtkSpinButton" id="refresh_interval">
                                <property name="visible">1</property>
                                <property name="can_focus">1</property>
                                <property name="numeric">1</property>
                                <property name="value">1</property>
                                <property name="adjustment">refresh_rate_adjustment</property>
                                <property name="valign">center</property>
                                <signal name="value-changed" handler="refresh_interval_changed" swapped="no" />
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>

                <!-- Show Percent Sign -->
                <child>
                  <object class="GtkListBoxRow">
                    <property name="visible">True</property>
                    <property name="can-focus">True</property>

                    <child>
                      <object class="GtkBox">
                        <property name="visible">True</property>
                        <property name="can-focus">False</property>
                        <property name="margin-start">12</property>
                        <property name="margin-end">12</property>
                        <property name="margin-top">12</property>
                        <property name="margin-bottom">12</property>
                        <property name="orientation">vertical</property>

                        <child>
                          <object class="GtkBox">
                            <property name="visible">True</property>
                            <property name="can-focus">False</property>
                            <property name="spacing">32</property>

                            <child>
                              <object class="GtkLabel">
                                <property name="visible">True</property>
                                <property name="can-focus">False</property>
                                <property name="valign">center</property>
                                <property name="label" translatable="yes">Show Percent Sign</property>
                                <property name="xalign">0</property>
                                <property name="hexpand">1</property>
                              </object>
                            </child>

                            <child>
                              <object class="GtkSwitch" id="show_percent_sign_switch">
                                <property name="visible">1</property>
                                <property name="can_focus">1</property>
                                <property name="halign">center</property>
                                <property name="valign">center</property>
                                <signal name="state-set" handler="show_percent_sign_switch_changed" swapped="no" />
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>

              </object>
            </child>
          </object>
        </child>

        <child>
          <object class="GtkButton">
            <property name="label" translatable="yes">Reset Settings To Default</property>
            <property name="visible">1</property>
            <property name="can_focus">1</property>
            <property name="receives_default">1</property>
            <signal name="clicked" handler="reset_settings_to_default_clicked" swapped="no" />
          </object>
        </child>

      </object>
    </child>
  </template>
</interface>